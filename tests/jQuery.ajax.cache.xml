<ModulePrefs title="cache - jQuery.ajax - opensocial-jquery">
</ModulePrefs>
<Content type="html"><![CDATA[
<script type="text/javascript" src="http://developmentor.lrlab.to/opensocial-jquery/jquery.js"></script>
<script type="text/javascript">
jQuery(function($) {

  var baseUrl = 'http://developmentor.lrlab.to/opensocial-jquery';
  
  /**
   * success
   */
   
  var getUrl = baseUrl + '/html.cgi';

  $.ajax({
    url: getUrl, // cache: undefined
	
	complete: function(xhr, status) {
      console.log('complete: async=true', xhr, status);
	}
  });

  var getUrl = baseUrl + '/html.cgi';

  $.ajax({ 
    url: getUrl, cache: true,
	
	complete: function(xhr, status) {
      console.log('complete: async=false', xhr, status);
	}
  });

  var getUrl = baseUrl + '/html.cgi';

  $.ajax({ 
    url: getUrl, cache: false,
	
	complete: function(xhr, status) {
      console.log('complete: async=false', xhr, status);
	}
  });

  var getUrl = baseUrl + '/script.cgi';

  $.ajax({ 
    url: getUrl, dataType: 'script', // cache: false (force)
	
	complete: function(xhr, status) {
      console.log('complete: async=false', xhr, status);
	}
  });

  $.ajax({ 
    url: getUrl, dataType: 'script', type: 'POST', // ignore cache:
	
	complete: function(xhr, status) {
      console.log('complete: async=false', xhr, status);
	}
  });

});
//gadgets.ut
</script>
<div id="event1"></div>
]]></Content>
</Module>
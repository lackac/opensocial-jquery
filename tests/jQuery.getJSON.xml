<Content type="html"><![CDATA[
<script type="text/javascript" src="http://developmentor.lrlab.to/opensocial-jquery/jquery.js"></script>
<script type="text/javascript">
jQuery(function($) {
  
  // Global Events
  $('#event1')
    .ajaxStart(function() { console.log('ajaxStart'); })
    .ajaxSend(function() { console.log('ajaxSend'); })
    .ajaxSuccess(function() { console.log('ajaxSuccess'); })
    .ajaxError(function() { console.log('ajaxError'); })
    .ajaxComplete(function() { console.log('ajaxComplete'); })
    .ajaxStop(function() { console.log('ajaxStop'); });

  /**
   * success -- JSON
   */

  // text/html
  // application/json
  // text/javascript
  var getUrl = 'http://developmentor.lrlab.to/opensocial-jquery/json.cgi';

  // GET
  $.getJSON(getUrl);
  
  // GET + callback
  $.getJSON(getUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

  // GET + QUERY_STRING + callback
  $.getJSON(getUrl, { foo: 'bar' }, function(data, status) {
    console.info(data);
    console.info(status);
  });

  /**
   * error -- JSON
   */

  var notJsonUrl = 'http://developmentor.lrlab.to/opensocial-jquery/parsererror.json';

  $.getJSON(notJsonUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

  var notfoundUrl = 'http://developmentor.lrlab.to/opensocial-jquery/404.html';

  $.getJSON(notfoundUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

  /**
   * success -- JSONP
   */

  var getUrl = 'http://developmentor.lrlab.to/opensocial-jquery/json.cgi?callback=?';

  // GET
  $.getJSON(getUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

  var getUrl = 'http://developmentor.lrlab.to/opensocial-jquery/json.cgi';

  // GET + QUERY_STRING + callback
  $.getJSON(getUrl, 'callback=?', function(data, status) {
    console.info(data);
    console.info(status);
  });

  /**
   * error -- JSONP
   */

  var notJsonUrl = 'http://developmentor.lrlab.to/opensocial-jquery/parsererror.json?callback=?';

  $.getJSON(notJsonUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

  var notfoundUrl = 'http://developmentor.lrlab.to/opensocial-jquery/404.html?callback=?';

  $.getJSON(notfoundUrl, function(data, status) {
    console.info(data);
    console.info(status);
  });

});
//gadgets.ut
</script>
<div id="event1"></div>

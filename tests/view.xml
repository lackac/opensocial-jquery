<ModulePrefs title="View - opensocial-jquery">
<Require feature="views" />
</ModulePrefs>
<Content type="html" view="home,canvas"><![CDATA[
<script type="text/javascript" src="http://developmentor.lrlab.to/opensocial-jquery/jquery.js"></script>
<script type="text/javascript" src="http://developmentor.lrlab.to/opensocial-jquery/jquery.gadgets.js"></script>
<script type="text/javascript">
jQuery(function($, data) {

  console.info($.view());
  console.info(data);
  console.info($.views());

  $('#canvas').click(function() {
     $.view('canvas');
  });

  $('#canvas_data').click(function() {
     $.view('canvas', { foo: 'bar' });
  });

});

jQuery(document).ready(function($, data) {
  console.info($.view());
  console.info(data);
  console.info($.views());
});

//gadgets.ut
</script>
<ul>
<li><a id="canvas">canvas</a></li>
<li><a id="canvas_data">canvas + data</a></li>
</ul>
]]></Content>
</Module>
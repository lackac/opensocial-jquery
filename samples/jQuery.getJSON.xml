<ModulePrefs 
  title="jQuery.getJSON - samples - opensocial-jquery"
>
</ModulePrefs>
<Content type="html"><![CDATA[
<script type="text/javascript" src="http://developmentor.lrlab.to/opensocial-jquery/jquery.js"></script>
<script type="text/javascript">
jQuery(function($) {

  var tag = 'dog';
  
  var url = 'http://gdata.youtube.com/feeds/videos?'
    + $.param({ vq: tag, 'max-results': 20, alt: 'json'});

  $.getJSON(url, function(data) {
    $.each(data.feed.entry, function() {
	  $('<img/>')
	    .attr('src', this.media$group.media$thumbnail[0].url)
	    .attr('width', 64)
		.attr('heieht', 48)
		.appendTo('#videos');
	});
  });

});
//gadgets.ut
</script>
<style type="text/css">
<!--
#videos img {
  margin: 2px;
}
-->
</style>
<div id="videos"></div>
]]></Content>
</Module>